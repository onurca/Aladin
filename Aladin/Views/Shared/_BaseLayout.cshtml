<!DOCTYPE html>
<html lang="en">
@using Helpers = Aladin.Helpers;

<head>
    <meta charset="utf-8" />
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>
        @ViewBag.Settings.PageTitle
    </title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link href="~/Content/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Content/vendor/jquery/jquery.min.js"></script>
    @RenderSection("Assets", required: false)
    <link href="~/Content/Site.css?v=@SystemVersion.Get()" rel="stylesheet" />
    <script src="~/Scripts/Common/fox.js"></script>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-114124630-3"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        //function gtag() { dataLayer.push(arguments); }
        //gtag('js', new Date());

        //gtag('config', 'UA-114124630-3');
        window.version = "@Helpers.SystemVersion.Get()";
    </script>
</head>

@RenderBody()

</html>